{
    "name": "ng-eslint",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "prelint-task": "prettier --write .",
        "lint-report": "eslint .",
        "lint-task": "eslint . --fix",
        "lint": "pnpm run prelint-task && pnpm run lint-task",
        "e2e": "ng e2e"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged --relative"
        }
    },
    "lint-staged": {
        "*.{js,ts,component.html}": [
            "eslint --fix"
        ]
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.3.10",
        "@angular/common": "~13.3.10",
        "@angular/core": "~13.3.10",
        "@angular/forms": "~13.3.10",
        "@angular/platform-browser": "~13.3.10",
        "@angular/platform-browser-dynamic": "~13.3.10",
        "@angular/router": "~13.3.10",
        "rxjs": "~6.6.7",
        "tslib": "2.4.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-builders/jest": "^13.0.4",
        "@angular-devkit/build-angular": "13.3.7",
        "@angular-eslint/builder": "13.2.1",
        "@angular-eslint/eslint-plugin": "13.2.1",
        "@angular-eslint/eslint-plugin-template": "13.2.1",
        "@angular-eslint/schematics": "13.2.1",
        "@angular-eslint/template-parser": "13.2.1",
        "@angular/cli": "13.3.7",
        "@angular/compiler": "~13.3.10",
        "@angular/compiler-cli": "~13.3.10",
        "@angular/language-service": "~13.3.10",
        "@babel/core": "^7.17.5",
        "@babel/eslint-parser": "^7.17.0",
        "@types/eslint": "^8.4.1",
        "@types/jasminewd2": "~2.0.10",
        "@types/jest": "^27.4.1",
        "@types/node": "^12.20.46",
        "@typescript-eslint/eslint-plugin": "^5.13.0",
        "@typescript-eslint/parser": "^5.13.0",
        "babel-jest": "^27.5.1",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
        "babel-preset-env": "^1.7.0",
        "eslint": "^8.10.0",
        "eslint-plugin-align-assignments": "^1.1.2",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jest": "^26.1.1",
        "eslint-plugin-jest-formatting": "^3.1.0",
        "eslint-plugin-jsdoc": "^37.9.6",
        "eslint-plugin-optimize-regex": "^1.2.1",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-protractor": "^2.1.1",
        "eslint-plugin-rxjs": "5.0.2",
        "eslint-plugin-rxjs-angular": "2.0.0",
        "husky": "^8.0.1",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.2",
        "jest": "28.1.0",
        "jest-preset-angular": "12.0.1",
        "lint-staged": "12.4.2",
        "ng-packagr": "^13.2.1",
        "prettier": "^2.5.1",
        "protractor": "~7.0.0",
        "ts-jest": "27.1.5",
        "ts-node": "~10.6.0",
        "typescript": "4.6.x"
    }
}
